{
  "name": "ScoutVoiceApp",
  "version": "0.0.1",
  "private": true,
  "pre-commit": [
    "lint-with-error"
  ],
  "scripts": {
    "start": "npm run change-ip && node node_modules/react-native/local-cli/cli.js start",
    "change-ip": "./set-ip.sh",
    "lint": "node_modules/.bin/eslint src/* || true",
    "fix-lint": "node_modules/.bin/eslint --fix src/* || true",
    "lint-with-error": "node_modules/.bin/eslint src/*",
    "test": "jest"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-native": "0.39.2",
    "react-native-audio": "3.0.0",
    "react-native-google-signin": "0.8.1",
    "react-native-navigation": "next",
    "react-native-speech": "0.1.2",
    "react-native-vector-icons": "3.0.0",
    "react-native-webrtc": "0.54.6",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-persist": "^4.0.1",
    "redux-persist-transform-filter": "0.0.6",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-react-native": "^2.2.1",
    "jest": "18.1.0",
    "pre-commit": "^1.2.2",
    "react-native-mock": "^0.2.9",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "\\.test\\.js$"
  }
}
